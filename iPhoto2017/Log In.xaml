<Window x:Class="iPhoto2017.Log_In"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:iPhoto2017"
        mc:Ignorable="d"
        Title="Log In" Height="233.427" Width="430.899" Background="#3b5998" KeyDown="Window_KeyDown">
    <Canvas>
        <Label Content=" Name" Canvas.Left="38" Canvas.Top="39"  FontWeight="Bold" FontSize="20" Foreground="#dfe3ee"/>
        <TextBox x:Name="txtEName" Height="32" Canvas.Left="127" TextWrapping="Wrap"   Canvas.Top="44" Width="253" Background="#8b9dc3"  FontSize="20" FontWeight="Bold" TabIndex="1"/>
        <PasswordBox x:Name="passbox"  Canvas.Left="127" Height="32" Canvas.Top="119"  FontSize="20" FontWeight="Bold" Background="#8b9dc3"  Width="253"/>
        <Label Content=" Password" Foreground="#dfe3ee" Canvas.Left="19" Canvas.Top="114"  FontWeight="Bold" FontSize="20"/>

    </Canvas>
</Window>
